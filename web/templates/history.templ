package templates

import "github.com/rodrigomeireles/gpt-client/backend/models"

templ History(history []models.ChatMessage) {
	for _, message := range history {
		switch message.Role {
			case "user":
				<p>User: { message.Content } </p>
			case "assistant":
				<p class="text-silver_lake_blue">Model: { message.Content } </p>
		}
	}
}
